msader15.dll,,,1
msado15.dll,,,1
msadrh15.dll,,,1

[ADCInstall]
MSADCO.DLL,,,1
MSADCOR.DLL,,,1
MSADCE.DLL,,,1
MSADCER.DLL,,,1
msdaprst.dll,,,1

[MSINFSection]
IEDATA16.INF

[OLEDBInstall]
msdadc.dll,,,1
MSDAPS.DLL,,,1

[MSInstallReg]
