HKLM,%KEY_RUNONCE%,"DataComp_ASYNC",,"regsvr32.exe /s %24%\%ProgramF%\%CommonF%\%CFSystem%\%DefaultOLEDB%\MSDAPS.DLL"

[RenameAppDir]
