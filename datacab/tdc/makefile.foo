.SUFFIXES: .idl

.idl.h:
    $(MIDL) $(MIDL_FLAGS)\
!ifndef WIN16
    -Zp8 \
!else
    /Zp2 /DWIN16 /DDOS /DWIN $(MIDL_FLAGS) \
!endif
    $(INCPATH0) \
    -char unsigned \
    -ms_ext -c_ext \
    -proxy $*.c \
    -iid $*.c \
    -dlldata $O\dlldata.c \
    -cpp_cmd $(TARGET_CPP) \
    -header $*.h \
    -tlb $*.tlb \
    $(C_DEFINES) \
    $(MIDL_OPTIMIZATION) \
    $O\$*.idl
