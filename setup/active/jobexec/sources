!IF 0

This directory is a drop point for binaries (dll, lib, exe) that are
not built, but need to be binplaced.

!ENDIF

TARGETPATH=
TARGETNAME=
TARGETTYPE=NOTARGET
SOURCES=

#VERIFY_LC=1
LC_PATH=($PROJECT_ROOT)\lcinf

# makefile.def will binplace any file listed in the MISCFILES macro.

!if $(386)
TDIR = x86
!else
TDIR = $(TARGET_DIRECTORY)
!endif

#!if $(FREEBUILD)
BDIR = retail
#!else
#BDIR = debug
#!endif

# Platform specific MISCFILES

X86_MISCFILES = 

MIPS_MISCFILES =

PPC_MISCFILES =

ALPHA_MISCFILES =

# Files that have the same name, but are different for each platform

COMMON_MISCFILES = \
!if "$(BUILD_PRODUCT)" != "IE"
    $(TDIR)\jgaw400.dll \
    $(TDIR)\jgdw400.dll \
    $(TDIR)\jgmd400.dll \
    $(TDIR)\jgpl400.dll \
    $(TDIR)\jgsd400.dll \
    $(TDIR)\jgsh400.dll \
!endif
!if "$(BUILD_PRODUCT)" != "NT"
    $(TDIR)\qtplugin.ocx \
!endif
    $(TDIR)\jobexec.dll \
    $(TDIR)\swflash.ocx


# Complete file lists

!if $(386)
MISCFILES = \
    $(MISCFILES) \
    $(COMMON_MISCFILES) \
    $(X86_MISCFILES)

!elseif $(MIPS)
MISCFILES = \
    $(MISCFILES) \
    $(COMMON_MISCFILES) \
    $(MIPS_MISCFILES)
8
!elseif $(PPC)
MISCFILES = \
    $(MISCFILES) \
    $(COMMON_MISCFILES) \
    $(PPC_MISCFILES)

!elseif $(ALPHA)
MISCFILES = \
    $(MISCFILES) \
    $(COMMON_MISCFILES) \
    $(ALPHA_MISCFILES)
!endif
