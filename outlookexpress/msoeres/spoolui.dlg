                    7,46,250,11
    DEFPUSHBUTTON   "&Hide",IDC_SP_MINIMIZE,264,7,50,14
    PUSHBUTTON      "&Stop",IDC_SP_STOP,264,25,50,14
    PUSHBUTTON      "&Details >>",IDC_SP_DETAILS,264,44,50,14
    CONTROL         "Animate1",IDC_SP_ANIMATE,ANIMATE_CLASS,ACS_CENTER |
