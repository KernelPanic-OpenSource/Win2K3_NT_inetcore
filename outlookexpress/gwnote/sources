ATHROOT=..
R=.
!include $(R)\common.inc
SOURCES_USED=$(SOURCES_USED) $(R)\common.inc

USE_LIBCMT=1
NO_NTDLL=1
UMTYPE=windows

TARGETPATH=obj
TARGETTYPE=DYNLINK
TARGETNAME=gwnote

DLLDEF=$O\gwnote.def
DLLENTRY=_DllMainCRTStartup

SYNCHRONIZE_DRAIN=1

INCLUDES=$(INCLUDES);$(BASEDIR)\private\windows\inc

LINKLIBS = \
    $(ATHROOT)\staticrt\$O\staticrt.lib

SOURCES= \
    gwnote.rc \
    init.cpp	 \
    envhost.cpp  \
    defguid.cpp  \
    dllmain.cpp  \
    privunk.cpp  \
    factory.cpp

TARGETLIBS= \
    $(BASEDIR)\public\sdk\lib\*\kernel32.lib \
    $(BASEDIR)\public\sdk\lib\*\ole32.lib \
    $(BASEDIR)\public\sdk\lib\*\user32.lib \
    $(BASEDIR)\public\sdk\lib\*\advapi32.lib \
    $(BASEDIR)\public\sdk\lib\*\shell32.lib \
    $(BASEDIR)\public\sdk\lib\*\gdi32.lib \
    $(BASEDIR)\public\sdk\lib\*\uuid.lib \
    $(BASEDIR)\public\sdk\lib\*\shlwapi.lib \
    $(BASEDIR)\public\sdk\lib\*\comdlg32.lib \
    $(ATHROOT)\msoert\$O\msoert2.lib \
    $(ATHROOT)\inetcomm\build\$O\inetcomm.lib

MISCFILES=

PRECOMPILED_CXX=1
!ifndef WIN16
PRECOMPILED_INCLUDE=pch.hxx
PRECOMPILED_OPTION=
PRECOMPILED_TARGET=
!else
PRECOMPILED_OPTION=/fh=$(O)\pch.pch
MAKEDLL=1
UMDEF=$(TARGET)
UMRES=$(O)\gwnote.res
!endif
