                    $(CRYPTDLG_PATH)\obj\mppc\view.obj
    $(EXDEV)\mpc\bin\lib -nodefaultlib -out:$(CRYPTDLG_RETAIL) $(CRYPTDLG_PATH)\obj\mppc\certfram.obj \
                        $(CRYPTDLG_PATH)\obj\mppc\demand.obj $(CRYPTDLG_PATH)\obj\mppc\fineprnt.obj \
                        $(CRYPTDLG_PATH)\obj\mppc\ltrust.obj $(CRYPTDLG_PATH)\obj\mppc\pch.obj \
