ATHROOT        = ..
EXC_DROP       = $(_NTROOT)\exc_drop

CRYPTDLG_PATH  = $(ATHROOT)\cryptdlg
MSOERT_PATH    = $(ATHROOT)\msoert
INETCOMM_PATH  = $(ATHROOT)\inetcomm

CRYPTDLG_RETAIL     = .\retail\cryptdlg.lib
CRYPTDLG_DEBUG      = .\debug\cryptdlg.lib
CRYPTDLG_RES_RETAIL = .\retail\cryptdlg.res
CRYPTDLG_RES_DEBUG  = .\debug\cryptdlg.res

MSOERT_RETAIL = .\retail\msoert2.lib
MSOERT_DEBUG  = .\debug\msoert2.lib

INETCOMM_RETAIL     = .\retail\inetcomm.lib
INETCOMM_DEBUG      = .\debug\inetcomm.lib
INETCOMM_RES_RETAIL = .\retail\inetcomm.res
INETCOMM_RES_DEBUG  = .\debug\inetcomm.res


all: $(CRYPTDLG_RETAIL) $(CRYPTDLG_DEBUG) $(CRYPTDLG_RES_RETAIL) $(CRYPTDLG_RES_DEBUG) $(MSOERT_RETAIL) $(MSOERT_DEBUG) \
                        $(INETCOMM_RETAIL) $(INETCOMM_DEBUG) $(INETCOMM_RES_RETAIL) $(INETCOMM_RES_DEBUG)
    xcopy /d $(CRYPTDLG_RETAIL) $(EXC_DROP)\rtl\usa
    xcopy /d $(CRYPTDLG_DEBUG) $(EXC_DROP)\dbg\usa
    xcopy /d $(CRYPTDLG_RES_RETAIL) $(EXC_DROP)\rtl\usa
    xcopy /d $(CRYPTDLG_RES_DEBUG) $(EXC_DROP)\dbg\usa
    xcopy /d $(MSOERT_RETAIL) $(EXC_DROP)\rtl\usa
    xcopy /d $(MSOERT_DEBUG) $(EXC_DROP)\dbg\usa
    xcopy /d $(INETCOMM_RETAIL) $(EXC_DROP)\rtl\usa
    xcopy /d $(INETCOMM_DEBUG) $(EXC_DROP)\dbg\usa
    xcopy /d $(INETCOMM_RES_RETAIL) $(EXC_DROP)\rtl\usa
    xcopy /d $(INETCOMM_RES_DEBUG) $(EXC_DROP)\dbg\usa
    xcopy /d .\retail\*.rlb $(EXC_DROP)\rtl\usa
    xcopy /d .\debug\*.rlb $(EXC_DROP)\dbg\usa

$(CRYPTDLG_RETAIL): $(CRYPTDLG_PATH)\obj\mppc\certfram.obj $(CRYPTDLG_PATH)\obj\mppc\demand.obj \
