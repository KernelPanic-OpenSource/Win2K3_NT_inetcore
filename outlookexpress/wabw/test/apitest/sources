R=..\..
!include $(R)\common.inc
SOURCES_USED=$(SOURCES_USED) $(R)\common.inc

TARGETNAME=apitest
TARGETPATH=obj
TARGETTYPE=PROGRAM

!if $(386)
USE_NOLIBS=1
!else
USE_NTDLL=1
!endif

TARGETLIBS= \
!if !$(386)
    $(BASEDIR)\public\sdk\lib\*\ntdll.lib \
!endif
    $(BASEDIR)\public\sdk\lib\*\mapi32.lib \
    $(BASEDIR)\public\sdk\lib\*\comdlg32.lib \
    $(BASEDIR)\public\sdk\lib\*\wab32.lib

SOURCES = \
    apitest.c     \
    dbgutil.c	  \
    instring.c    \
    wabguid.c     \
    mapi.c 	  \
    apitest.rc

UMTYPE=windows
UMENTRY=winmain
