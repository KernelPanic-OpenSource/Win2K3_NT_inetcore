    $(BASEDIR)\public\sdk\lib\*\uuid.lib \
    $(BASEDIR)\public\sdk\lib\*\shell32.lib \
    $(BASEDIR)\public\sdk\lib\*\libcmt.lib \
    $(BASEDIR)\public\sdk\lib\*\version.lib


