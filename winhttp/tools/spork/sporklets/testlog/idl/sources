TARGETPATH=obj
TARGETTYPE=library
TARGETNAME=testlog

NO_BROWSER_FILE=1
INCLUDES=.;$(O)

# These are used in makefile.def when building the idl files.
PASS0_HEADERDIR = $(O)
PASS0_SOURCEDIR = $(O)
MIDL_UUIDDIR    = $(O)
MIDL_TLBDIR     = ..\dll
#MIDL_NO_ROBUST = 1


SOURCES= \
    testlog.idl
    
PASS0_PUBLISH= \
    {$(O)\testlog.h=..\inc\ifaces.h} \
    {$(O)\testlog_i.c=..\dll\guids.c}
